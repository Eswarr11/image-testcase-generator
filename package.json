{
  "name": "jira-test-case-generator-web",
  "version": "2.0.0",
  "description": "A TypeScript web application with React frontend that uses OpenAI's GPT-4o capabilities to generate Jira test cases from prompts and optional images.",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsx watch src/server.ts",
    "dev:client": "cd client && npm run dev",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc",
    "build:client": "cd client && npm run build",
    "start": "NODE_ENV=production node dist/server.js",
    "clean": "rm -rf dist && cd client && rm -rf dist",
    "lint": "eslint src/**/*.ts && cd client && npm run lint",
    "lint:fix": "eslint src/**/*.ts --fix && cd client && npm run lint:fix",
    "type-check": "tsc --noEmit && cd client && npm run type-check",
    "test": "echo 'Tests not yet configured'",
    "postinstall": "cd client && npm install"
  },
  "keywords": [
    "typescript",
    "react",
    "tailwind",
    "web-application",
    "jira",
    "test-cases",
    "openai",
    "gpt-4o",
    "qa",
    "testing",
    "automation",
    "ai-powered",
    "image-analysis",
    "quality-assurance",
    "webapp"
  ],
  "author": "Eswar A",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Eswarr11/image-testcase-generator.git"
  },
  "bugs": {
    "url": "https://github.com/Eswarr11/image-testcase-generator/issues"
  },
  "homepage": "https://github.com/Eswarr11/image-testcase-generator#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "express": "^4.19.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}
